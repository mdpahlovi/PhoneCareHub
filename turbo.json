{
    "$schema": "https://turbo.build/schema.json",
    "globalDotEnv": [".env"],
    "pipeline": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": [".next/**", "dist/**"],
            "dotEnv": [".env.production.local", ".env.local", ".env.production", ".env"]
        },
        "start": { "dependsOn": ["^start"] },
        "dev": { "dotEnv": [".env.development.local", ".env.local", ".env.development", ".env"] }
    }
}
