{
    "$schema": "https://turbo.build/schema.json",
    "globalDotEnv": [".env"],
    "pipeline": {
        "dev": {
            "cache": false,
            "persistent": true,
            "dotEnv": [".env.development.local", ".env.development", ".env.local", ".env"]
        },
        "build": {
            "dependsOn": ["^build"],
            "outputs": ["dist/**"],
            "dotEnv": [".env.production.local", ".env.production", ".env.local", ".env"]
        },
        "start": { "dependsOn": ["^start"] }
    }
}
